{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-ida",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/conf-ida": "*",
        "@opam-alpha/bap-ida-python": "*",
        "@opam-alpha/core_kernel": " >= 113.24.0",
        "@opam-alpha/oasis": " =0.4.7",
        "@opam-alpha/ppx_jane": " >= 113.24.1",
        "@opam-alpha/fileutils": "*",
        "@opam-alpha/re": "*",
        "@opam-alpha/bap-ida-plugin": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--enable-ida",
            "--ida-path=",
            "--ida-headless="
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_ida_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_ida_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_ida_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.0.0.tar.gz",
