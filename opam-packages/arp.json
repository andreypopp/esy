        "@opam-alpha/lwt": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "pkg/pkg.ml",
            "build",
            "--pinned",
            "false",
            "--with-mirage",
            "%{mirage-types+tcpip+lwt:installed}%"
