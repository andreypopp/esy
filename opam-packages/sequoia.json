{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/sequoia",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ppx_tools": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "sequoia_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "sequoia_installed": {
            "val": "true",
            "scope": "global"
          },
          "sequoia_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/andrenth/sequoia/archive/0.1.0.tar.gz",
        "checksum": "9a314cd5452224be688f2902f27bedb4",
        "files": []
      }
    }
  }
}