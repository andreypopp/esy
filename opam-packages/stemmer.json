{
  "versions": {
    "0.2.0": {
      "name": "@opam-alpha/stemmer",
      "version": "0.2.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "stemmer_version": {
            "val": "0.2.0",
            "scope": "global"
          },
          "stemmer_installed": {
            "val": "true",
            "scope": "global"
          },
          "stemmer_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/pymander/ocaml-stemmer/archive/v0.2.tar.gz",
        "checksum": "29588f4e3db0175e9a720143effd5f2b",
        "files": []
      }
    }
  }
}