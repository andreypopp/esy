{
  "versions": {
    "0.22.0": {
      "name": "@opam-alpha/numerix",
      "version": "0.22.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/conf-gmp": "*",
        "@opam-alpha/num": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--enable-sse2",
            "--disable-lang",
            "--enable-ocaml",
            "--enable-clong",
            "--enable-dlong",
            "--enable-slong",
            "--enable-gmp",
            "--enable-ocaml_bignum"
          ],
          [
            "make",
            "clean",
            "lib",
            "examples",
            "test"
          ],
          [
            "make",
            "install"
          ],
          [
            "mkdir",
            "-p",
            "$cur__install/doc/numerix/"
          ],
          [
            "cp",
            "doc/english/numerix.pdf",
            "$cur__install/doc/numerix/numerix-en.pdf"
          ],
          [
            "cp",
            "doc/francais/numerix.pdf",
            "$cur__install/doc/numerix/numerix-fr.pdf"
          ],
          [
            "echo",
            "*** Check that $cur__lib is in your LD_LIBRARY_PATH ***"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "numerix_version": {
            "val": "0.22.0",
            "scope": "global"
          },
          "numerix_installed": {
            "val": "true",
            "scope": "global"
          },
          "numerix_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://pauillac.inria.fr/~quercia/cdrom/bibs/numerix.tar.gz",
        "checksum": "a2169c2251508ee2fb891f878e17ef96",
        "files": []
      }
    }
  }
}