{
  "versions": {
    "0.3.0": {
      "name": "@opam-alpha/posix-semaphore",
      "version": "0.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/result": "*",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/posix-time": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "posix_semaphore_version": {
            "val": "0.3.0",
            "scope": "global"
          },
          "posix_semaphore_installed": {
            "val": "true",
            "scope": "global"
          },
          "posix_semaphore_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
