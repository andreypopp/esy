{
  "versions": {
    "0.22.1": {
      "name": "@opam-alpha/containers",
      "version": "0.22.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/base-bytes": "*",
        "@opam-alpha/result": "*",
        "@opam-alpha/cppo": "",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/sequence": "*",
        "@opam-alpha/base-bigarray": "*",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/base-threads": "*",
        "@opam-alpha/qtest": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install",
            "--${base_threads_enable:-disable}-thread",
            "--disable-bench",
            "--disable-tests",
            "--${base_bigarray_enable:-disable}-bigarray",
            "--${sequence_enable:-disable}-advanced",
            "--${base_unix_enable:-disable}-unix",
            "--enable-docs"
          ],
          [
            "make",
            "build"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "containers_version": {
            "val": "0.22.1",
            "scope": "global"
          },
          "containers_installed": {
            "val": "true",
            "scope": "global"
          },
          "containers_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/c-cube/ocaml-containers/archive/0.22.1.tar.gz",
        "checksum": "491e2095ac76bdf97d8a8179712cc973",
        "files": []
      }
    }
  }
}