{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/opam-installext",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "install",
            "PREFIX=$cur__install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "opam_installext_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "opam_installext_installed": {
            "val": "true",
            "scope": "global"
          },
          "opam_installext_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/avsm/opam-installext/archive/v1.0.0.tar.gz",
        "checksum": "9ca1a7e3e7e10963b3f2dbca2297da13",
        "files": []
      }
    },
