        "@opam-alpha/ounit": "",
        "@opam-alpha/bisect": "",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "mirage_block_ccm_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "mirage_block_ccm_installed": {
            "val": "true",
            "scope": "global"
          },
          "mirage_block_ccm_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
