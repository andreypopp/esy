{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/elf2json",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base64": "  >= 2.0.0",
        "@opam-alpha/jsonm": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/rdr": " >= 2.0.0",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "elf2json_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "elf2json_installed": {
            "val": "true",
            "scope": "global"
          },
          "elf2json_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://github.com/m4b/elf2json/archive/v1.0.0.tar.gz",
        "checksum": "5c0e113305839b7825d7edc888f962be",
        "files": [
          {
            "name": "_oasis_remove_.ml",
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n"
          },
          {
            "name": "elf2json.install",
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n"
          }
        ]
      }
    }
  }
}