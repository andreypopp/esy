{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/maki",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/result": "*",
        "@opam-alpha/lwt": " >= 2.4.7",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/bencode": "*",
        "@opam-alpha/sha": "*",
        "@opam-alpha/base-threads": "*",
        "@opam-alpha/yojson": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install",
            "--${yojson_enable:-disable}-yojson"
          ],
          [
            "make",
            "build"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "maki_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "maki_installed": {
            "val": "true",
            "scope": "global"
          },
          "maki_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/c-cube/maki/archive/0.1.tar.gz",
        "checksum": "822e1f4248f874935a53d89040152e88",
