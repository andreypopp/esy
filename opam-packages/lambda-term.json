{
  "versions": {
    "1.9.0": {
      "name": "@opam-alpha/lambda-term",
      "version": "1.9.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/lwt": " >= 2.4.0  < 3.0.0",
        "@opam-alpha/zed": " >= 1.2.0",
        "@opam-alpha/react": " >= 1.0.0",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "CAML_LD_LIBRARY_PATH": {
            "val": "$opam_alpha__slash__lambda_term__lib/lambda-term:$CAML_LD_LIBRARY_PATH",
            "scope": "global"
          },
          "lambda_term_version": {
            "val": "1.9.0",
            "scope": "global"
          },
          "lambda_term_installed": {
            "val": "true",
            "scope": "global"
          },
          "lambda_term_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/diml/lambda-term/archive/1.9.tar.gz",
        "checksum": "17a9f8b56f9a10a7ed4cfa48ae6c3e39",
        "files": [
          {
            "name": "lambda-term.install",
            "content": "bin: [\n  \"?_build/tools/lambda_term_actions.byte\" {\"lambda-term-actions\"}\n  \"?_build/tools/lambda_term_actions.native\" {\"lambda-term-actions\"}\n]\n"
          }
        ]
      }
    }
  }
}