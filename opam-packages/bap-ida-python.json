{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-ida-python",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/bap": "=1.0.0",
        "@opam-alpha/conf-ida": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "substs",
            "bap.cfg.in"
          ],
          [
            "cp",
            "-v",
            "plugins/plugin_loader_bap.py",
            "/plugins/"
          ],
          [
            "cp",
            "-rv",
            "plugins/bap",
            "/plugins/bap"
          ],
          [
            "cp",
            "-v",
            "bap.cfg",
            "/cfg/"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_ida_python_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_ida_python_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_ida_python_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap-ida-python/archive/v1.0.0.tar.gz",
        "checksum": "041696dd7c83b43d63fea06af75fe65b",
        "files": [
          {
            "name": "bap.cfg.in",
            "content": ".default\nbap_executable_path    %{bap:bin}%/bap\n"
          }
        ]
      }
    },
