{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/merlin-acme",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/batteries": "*",
        "@opam-alpha/yojson": "*",
        "@opam-alpha/acme": "*",
        "@opam-alpha/ocaml9p": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "merlin_acme_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "merlin_acme_installed": {
            "val": "true",
            "scope": "global"
          },
          "merlin_acme_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/raphael-proust/merlin-acme/archive/0.1.tar.gz",
        "checksum": "3e8a2d4d12e3683d9a30b53d14e6a0c7",
        "files": [
          {
            "name": "merlin-acme.install",
            "content": "bin: [\n\t\"Errors\"\n\t\"TypeOf\"\n\t\"Locate\"\n\t\"Cplt\"\n]\n"
          }
        ]
      }
    }
  }
}