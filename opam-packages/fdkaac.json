{
  "versions": {
    "0.2.1": {
      "name": "@opam-alpha/fdkaac",
      "version": "0.2.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "./configure",
            "OCAMLFLAGS=-cclib -L/usr/local/lib",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "fdkaac_version": {
            "val": "0.2.1",
            "scope": "global"
          },
          "fdkaac_installed": {
            "val": "true",
            "scope": "global"
          },
          "fdkaac_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/savonet/ocaml-fdkaac/releases/download/0.2.1/ocaml-fdkaac-0.2.1.tar.gz",
        "checksum": "fff652fa282b4954d2f504ca1c2a70e9",
        "files": []
      }
    }
  }
}