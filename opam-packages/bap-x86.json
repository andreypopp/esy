{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-x86",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/bap-std": "*",
        "@opam-alpha/bap-abi": "*",
        "@opam-alpha/bap-c": "*",
        "@opam-alpha/bap-llvm": "*",
        "@opam-alpha/cmdliner": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--enable-x86"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_x86_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_x86_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_x86_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.0.0.tar.gz",
        "checksum": "07dce66dd871e448652d8885283c6631",
        "files": []
      }
    },
    "1.1.0": {
      "name": "@opam-alpha/bap-x86",
      "version": "1.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/bap-std": "*",
        "@opam-alpha/bap-abi": "*",
        "@opam-alpha/bap-c": "*",
        "@opam-alpha/bap-llvm": "*",
        "@opam-alpha/cmdliner": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "sh",
            "-x",
            "setup-llvm-version.sh"
          ],
          [
            "./configure",
            "--prefix=$cur__install",
            "--enable-x86"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_x86_version": {
            "val": "1.1.0",
            "scope": "global"
          },
          "bap_x86_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_x86_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.1.0.tar.gz",
        "checksum": "92e7f703d58ce1835bfeeed9ec523242",
        "files": [
          {
            "name": "setup-llvm-version.sh",
            "content": "VERSION=3.4\n\nif `opam config var conf-bap-llvm:installed` == \"true\"; then\n    VERSION=`opam config var conf-bap-llvm:version`\nelse\n    VERSION=`opam config var conf-llvm:version`\nfi\n\ncat > plugins/x86/x86_llvm_config.ml.ab <<EOF\nlet llvm_version = \"$VERSION\"\n\nEOF\n"
          }
        ]
      }
    }
  }
}