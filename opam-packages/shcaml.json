{
  "versions": {
    "0.1.3": {
      "name": "@opam-alpha/shcaml",
      "version": "0.1.3",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/pcre": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "shcaml_version": {
            "val": "0.1.3",
            "scope": "global"
          },
          "shcaml_installed": {
            "val": "true",
            "scope": "global"
          },
          "shcaml_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://godi.0ok.org/godi-backup/shcaml-0.1.3.tar.gz",
        "checksum": "0b8a1a03cc6fe314a13d846bd417a7a8",
        "files": []
      }
    }
  }
}