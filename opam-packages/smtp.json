{
  "versions": {
    "0.2.0": {
      "name": "@opam-alpha/smtp",
      "version": "0.2.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/lwt": "*",
        "@opam-alpha/base-unix": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "pkg/build.ml",
            "native=true",
            "native-dynlink=true",
            "unix=${base_unix_installed:-false}",
            "lwt=${lwt_installed:-false}"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "smtp_version": {
            "val": "0.2.0",
            "scope": "global"
          },
          "smtp_installed": {
            "val": "true",
            "scope": "global"
          },
          "smtp_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
