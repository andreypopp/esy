{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/spdiff",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/menhir": " <= 20140422.0.0"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "depend"
          ],
          [
            "make",
            "opt"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "spdiff_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "spdiff_installed": {
            "val": "true",
            "scope": "global"
          },
          "spdiff_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/jespera/spdiff/archive/0.1.tar.gz",
        "checksum": "d523b48a5e7b4d392cf7fcc7a2a07553",
        "files": [
          {
            "name": "spdiff.install",
            "content": "bin: [\n  \"spdiff.opt\" {\"spdiff\"}\n]\n"
          }
        ]
      }
    }
  }
}