{
  "versions": {
    "0.5.0": {
      "name": "@opam-alpha/functory",
      "version": "0.5.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure"
          ],
          [
            "make"
          ],
          [
            "make",
            "ocamlfind-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "functory_version": {
            "val": "0.5.0",
            "scope": "global"
          },
          "functory_installed": {
            "val": "true",
            "scope": "global"
          },
          "functory_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://www.lri.fr/~filliatr/functory/download/functory-0.5.tar.gz",
        "checksum": "c7e6576c3e6b3a7e247eab530d7e4de8",
        "files": []
      }
    }
  }
}