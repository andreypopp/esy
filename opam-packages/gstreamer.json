  "versions": {
    "0.2.0": {
      "name": "@opam-alpha/gstreamer",
      "version": "0.2.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "gstreamer_version": {
            "val": "0.2.0",
            "scope": "global"
          },
          "gstreamer_installed": {
            "val": "true",
            "scope": "global"
          },
          "gstreamer_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
