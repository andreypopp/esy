{
  "versions": {
    "1.19.1": {
      "name": "@opam-alpha/gsl",
      "version": "1.19.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base-bigarray": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/conf-gsl": "*",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/camlp4": "esy-ocaml/camlp4#esy/4.02+7-TEST"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install",
            "--${camlp4_enable:-disable}-camlp4"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "gsl_version": {
            "val": "1.19.1",
            "scope": "global"
          },
          "gsl_installed": {
            "val": "true",
            "scope": "global"
          },
          "gsl_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mmottl/gsl-ocaml/releases/download/v1.19.1/gsl-ocaml-1.19.1.tar.gz",
