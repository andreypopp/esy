{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/ppx_curried_constr",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/omake": "  < 0.10.1"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ppx___curried___constr_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "ppx___curried___constr_installed": {
            "val": "true",
            "scope": "global"
          },
          "ppx___curried___constr_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://bitbucket.org/camlspotter/ppx_curried_constr/get/1.0.0.tar.gz",
        "checksum": "5360d9f6c68056c66a46ccf9c680d4e1",
        "files": []
      }
    }
  }
}