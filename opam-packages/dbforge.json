{
  "versions": {
    "2.0.0": {
      "name": "@opam-alpha/dbforge",
      "version": "2.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/config-file": " >= 1.1.0",
        "@opam-alpha/lablgtk-extras": " >= 1.2.0",
        "@opam-alpha/lablgtk": " >= 2.16.0",
        "@opam-alpha/mysql": "*",
        "@opam-alpha/xml-light": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "dbforge_version": {
            "val": "2.0.0",
            "scope": "global"
          },
          "dbforge_installed": {
            "val": "true",
            "scope": "global"
          },
          "dbforge_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://zoggy.github.com/dbforge/dbforge-2.0.tar.gz",
        "checksum": "78660bfc79e6b2e374ca992ca6ad887f",
        "files": [
          {
            "name": "dbforge.install",
            "content": "bin: [\n  \"?src/dbforge\"\n  \"?src/dbforge.byte\"\n  \"?src/dbforge-gtk\"\n  \"?src/dbforge-gtk.byte\"\n]\n"
          }
        ]
      }
    },
