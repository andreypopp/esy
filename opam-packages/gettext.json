{
  "versions": {
    "0.3.4": {
      "name": "@opam-alpha/gettext",
      "version": "0.3.4",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ounit": "*",
        "@opam-alpha/fileutils": "*",
        "@opam-alpha/camomile": "*",
        "@opam-alpha/camlp4": "esy-ocaml/camlp4#esy/4.02+7-TEST"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--disable-doc",
            "--prefix",
            "$cur__install",
            "--sbindir=$cur__lib/gettext/sbin",
            "--libexecdir=$cur__lib/gettext/libexec",
            "--sysconfdir=$cur__lib/gettext/etc",
            "--sharedstatedir=$cur__lib/gettext/com",
            "--localstatedir=$cur__lib/gettext/var",
            "--libdir=$cur__lib/gettext/lib",
            "--includedir=$cur__lib/gettext/include",
            "--datarootdir=$cur__lib/gettext/share"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "gettext_version": {
            "val": "0.3.4",
            "scope": "global"
          },
          "gettext_installed": {
            "val": "true",
            "scope": "global"
          },
          "gettext_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/676/ocaml-gettext-0.3.4.tar.gz",
        "checksum": "2c588ba92e9a809f2885ecacc48069a9",
        "files": [
          {
            "name": "gettext.install",
            "content": "bin: [\n  \"_build/bin/ocaml-gettext\"\n  \"_build/bin/ocaml-xgettext\"\n]\n"
          }
        ]
      }
    },
