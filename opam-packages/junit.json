{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/junit",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ptime": "*",
        "@opam-alpha/tyxml": " >= 4.0.0"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "junit_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "junit_installed": {
            "val": "true",
            "scope": "global"
          },
          "junit_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/Khady/ocaml-junit/archive/0.1.zip",
        "checksum": "b25790f915ed08bd87796b3d722e0fd0",
        "files": []
      }
    }
  }
}