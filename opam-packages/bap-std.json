{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-std",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/bap-future": "*",
        "@opam-alpha/camlzip": "*",
        "@opam-alpha/core_kernel": " >= 113.33.0",
        "@opam-alpha/fileutils": "*",
        "@opam-alpha/graphlib": "*",
        "@opam-alpha/oasis": " =0.4.7",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ppx_jane": " >= 113.33.0  < 113.34.0",
        "@opam-alpha/regular": "*",
        "@opam-alpha/uri": "*",
        "@opam-alpha/utop": "*",
        "@opam-alpha/uuidm": "*",
        "@opam-alpha/zarith": "*",
        "@opam-alpha/cmdliner": " <= 0.9.8"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--with-cxx=`which clang++`",
            "--mandir=$cur__man",
            "--enable-bap-std"
          ],
          [
            "make"
          ],
          [
            "make",
            "reinstall"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_std_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_std_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_std_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.0.0.tar.gz",
        "checksum": "07dce66dd871e448652d8885283c6631",
        "files": []
      }
    },
