{
  "versions": {
    "1.3.0": {
      "name": "@opam-alpha/camlmix",
      "version": "1.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "install",
            "PREFIX=$cur__install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "camlmix_version": {
            "val": "1.3.0",
            "scope": "global"
          },
          "camlmix_installed": {
            "val": "true",
            "scope": "global"
          },
          "camlmix_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mjambon/mjambon2016/raw/master/camlmix/camlmix-1.3.0.tar.gz",
        "checksum": "abaf0152bfc786398c2c4f9176a789a8",
        "files": [
          {
            "name": "camlmix.install",
            "content": "bin: [\"camlmix\"]\n"
          }
        ]
      }
    }
  }
}