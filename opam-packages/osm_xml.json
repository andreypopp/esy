{
  "versions": {
    "0.0.1": {
      "name": "@opam-alpha/osm_xml",
      "version": "0.0.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/core": "*",
        "@opam-alpha/xmlm": "*",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/ounit": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--${ounit_enable:-disable}-tests",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "osm___xml_version": {
            "val": "0.0.1",
            "scope": "global"
          },
          "osm___xml_installed": {
            "val": "true",
            "scope": "global"
          },
          "osm___xml_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/aluuu/osm_xml/archive/v0.0.1.tar.gz",
        "checksum": "f6ef69812168f3946bc9234358f4f268",
        "files": []
      }
    }
  }
}