{
  "versions": {
    "0.3.0": {
      "name": "@opam-alpha/gdb",
      "version": "0.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/menhir": "*",
        "@opam-alpha/extlib": "*",
        "@opam-alpha/extlib-compat": "*",
        "@opam-alpha/lwt": " >= 2.4.6",
        "@opam-alpha/ppx_deriving": " >= 1.0.0",
        "@opam-alpha/ppx_tools": "  >= 0.99.1",
        "@opam-alpha/oasis": "  >= 0.4.0",
        "@opam-alpha/cppo": "",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/lambda-term": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--${lambda_term_enable:-disable}-build-rmp",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "gdb_version": {
            "val": "0.3.0",
            "scope": "global"
          },
          "gdb_installed": {
            "val": "true",
            "scope": "global"
          },
          "gdb_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/ygrek/ocaml-gdb/archive/0.3.tar.gz",
        "checksum": "a9f44ae88ef35704b0deb3aa37b2a5ed",
        "files": [
          {
            "name": "gdb.install",
            "content": "bin: [\n  \"?_build/src/rmp.native\" {\"rmp\"}\n]\n"
          }
        ]
      }
    }
  }
}