        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "pkg/build.ml",
            "native=true",
            "native-dynlink=true",
            "lwt=${lwt_installed:-false}",
            "ssl=${ssl_installed:-false}"
