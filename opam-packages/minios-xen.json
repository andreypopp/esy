    "0.9.0": {
      "name": "@opam-alpha/minios-xen",
      "version": "0.9.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/conf-perl": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "sh",
            "-ex",
            "./build-opam.sh"
          ],
          [
            "make",
            "install",
            "LIBDIR=$cur__install/lib",
            "INCLUDEDIR=$cur__install/include"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "minios_xen_version": {
