{
  "versions": {
    "1.0.1": {
      "name": "@opam-alpha/ocp-pack-split",
      "version": "1.0.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocp_pack_split_version": {
            "val": "1.0.1",
            "scope": "global"
          },
          "ocp_pack_split_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocp_pack_split_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://www.ocamlpro.com//files/ocp-pack-1.0.1.tar.gz",
        "checksum": "9122bc296089330db082965152164ce7",
        "files": [
          {
            "name": "ocp-pack-split.install",
            "content": "bin: [\n  \"ocp-pack\" {\"ocp-pack\"}\n  \"ocp-split\" {\"ocp-split\"}\n]\n"
          }
        ]
      }
    }
  }
}