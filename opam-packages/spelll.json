{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/spelll",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--docdir",
            "$cur__doc/spelll/"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "spelll_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "spelll_installed": {
            "val": "true",
            "scope": "global"
          },
          "spelll_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/c-cube/spelll/archive/0.1.tar.gz",
        "checksum": "e4574039054e3850357ea434dfcac039",
        "files": []
      }
    }
  }
}