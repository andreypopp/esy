{
  "versions": {
    "0.1.1": {
      "name": "@opam-alpha/mirage-console-solo5",
      "version": "0.1.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/mirage-console": "*",
        "@opam-alpha/mirage-types-lwt": " < 3.0.0",
        "@opam-alpha/mirage-solo5": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "PREFIX=$cur__install"
          ],
          [
            "make",
            "install",
            "PREFIX=$cur__install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "mirage_console_solo5_version": {
            "val": "0.1.1",
            "scope": "global"
          },
          "mirage_console_solo5_installed": {
            "val": "true",
            "scope": "global"
          },
          "mirage_console_solo5_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mirage/mirage-console-solo5/archive/v0.1.1.tar.gz",
        "checksum": "e6749a92ac0ed788d8be54094565a039",
        "files": []
      }
    }
  }
