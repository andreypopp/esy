        "files": [
          {
            "name": "disable-ocamldep-native.patch",
            "content": "diff --git a/Makefile.in b/Makefile.in\nindex 8c9d6ca..2c2b090 100644\n--- a/Makefile.in\n+++ b/Makefile.in\n@@ -64,7 +64,8 @@ endif\n \n DEPFLAGS  = -slash -I lib/why3\n ifeq (@OCAMLBEST@,opt)\n-DEPFLAGS += -native\n+# the semantics of the -native flag changed in ocaml 4.03.0\n+#DEPFLAGS += -native\n endif\n \n RUBBER = @RUBBER@\n"
