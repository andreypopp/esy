{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/erssical",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/xmlm": "*",
        "@opam-alpha/xtmpl": " > 0.4.0  <= 0.6.0",
        "@opam-alpha/ocamlnet": " >= 3.6.5  <= 3.7.7",
        "@opam-alpha/ocamlrss": " >= 2.2.1",
        "@opam-alpha/ocurl": " >= 0.5.6"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "BINDIR=$cur__bin",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "erssical_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "erssical_installed": {
            "val": "true",
            "scope": "global"
          },
          "erssical_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://zoggy.github.io/erssical/erssical-0.1.tar.gz",
        "checksum": "af39093d3472dbee0363c0e5255d086c",
        "files": []
      }
    }
  }
}