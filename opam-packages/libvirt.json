{
  "versions": {
    "0.6.1": {
      "name": "@opam-alpha/libvirt",
      "version": "0.6.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure"
          ],
          [
            "env",
            "CPPFLAGS=-Wno-error=tautological-compare -Wno-error=unused-function",
            "make",
            "all"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "opt"
          ],
          [
            "make",
            "install-opt"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "libvirt_version": {
            "val": "0.6.1",
            "scope": "global"
          },
          "libvirt_installed": {
            "val": "true",
            "scope": "global"
          },
          "libvirt_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://libvirt.org/sources/ocaml/ocaml-libvirt-0.6.1.4.tar.gz",
        "checksum": "92723c155c009880475f3c9a093d1fe6",
        "files": []
      }
    }
  }
}