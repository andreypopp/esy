{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/cordova-plugin-media",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/gen_js_api": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "build"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "cordova_plugin_media_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "cordova_plugin_media_installed": {
            "val": "true",
            "scope": "global"
          },
          "cordova_plugin_media_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/dannywillems/ocaml-cordova-plugin-media/archive/v1.0.tar.gz",
        "checksum": "7a2d5ebe58b4e528424b6291495939e7",
        "files": []
      }
    }
  }
}