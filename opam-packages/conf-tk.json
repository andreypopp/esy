{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/conf-tk",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/conf-pkg-config": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "sh",
            "check.sh"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "conf_tk_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "conf_tk_installed": {
            "val": "true",
            "scope": "global"
          },
          "conf_tk_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": null,
        "checksum": null,
        "files": [
          {
            "name": "check.sh",
            "content": "pkg-config tk || \\\ncc -o /dev/null \\\n  -I/usr/include/tk \\\n  -I/usr/local/include/tk \\\n  -I/opt/X11/include \\\n  compiletest.c\n"
          },
          {
            "name": "compiletest.c",
            "content": "#include <stdio.h>\n#include <tk.h>\nint main (){\n\tputs(TK_VERSION);\n\treturn 0;\n}\n"
          }
        ]
      }
    }
  }
}