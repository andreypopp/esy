{
  "versions": {
    "4.0.1": {
      "name": "@opam-alpha/ocamlspot",
      "version": "4.0.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "all",
            "opt",
            "install",
            "BINDIR=$cur__bin",
            "PREFIX=$cur__install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocamlspot_version": {
            "val": "4.0.1",
            "scope": "global"
          },
          "ocamlspot_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocamlspot_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
