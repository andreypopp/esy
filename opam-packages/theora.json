        "@opam-alpha/ogg": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "theora_version": {
            "val": "0.3.0",
            "scope": "global"
          },
          "theora_installed": {
            "val": "true",
            "scope": "global"
          },
          "theora_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
