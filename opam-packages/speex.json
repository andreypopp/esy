          [
            "./configure",
            "CFLAGS=-I/usr/local/include",
            "LDFLAGS=-L/usr/local/lib",
            "OCAMLFLAGS=-ccopt -I/usr/local/include -cclib -L/usr/local/lib",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "speex_version": {
            "val": "0.2.1",
            "scope": "global"
          },
          "speex_installed": {
            "val": "true",
            "scope": "global"
          },
          "speex_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
