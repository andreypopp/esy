{
  "versions": {
    "0.5.0": {
      "name": "@opam-alpha/sibylfs",
      "version": "0.5.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/base-bytes": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/sibylfs-lem": "",
        "@opam-alpha/cppo": "",
        "@opam-alpha/sha": "*",
        "@opam-alpha/cmdliner": "*",
        "@opam-alpha/fd-send-recv": "*",
        "@opam-alpha/camlp4": "esy-ocaml/camlp4#esy/4.02+7-TEST",
        "@opam-alpha/sexplib": " < 113.1.0",
        "@opam-alpha/menhir": "*",
        "@opam-alpha/cow": " >= 1.2.0  < 2.0.0",
        "@opam-alpha/unix-fcntl": " >= 0.2.0  < 0.3.0"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "mkdir",
            "-p",
            "src_ext/lem/ocaml-lib"
          ],
          [
            "ln",
            "-s",
            "sibylfs-lem:lib",
            "src_ext/lem/ocaml-lib/_build"
          ],
          [
            "ln",
            "-s",
            "sibylfs_lem__bin/lem",
            "src_ext/lem/lem"
          ],
          [
            "make"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "sibylfs_version": {
            "val": "0.5.0",
            "scope": "global"
          },
          "sibylfs_installed": {
            "val": "true",
            "scope": "global"
          },
          "sibylfs_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/sibylfs/sibylfs_src/archive/0.5.0.tar.gz",
        "checksum": "3ec26e4dcd63041162473cb15f563a48",
        "files": []
      }
    }
  }
}