{
  "versions": {
    "0.9.0": {
      "name": "@opam-alpha/zbar",
      "version": "0.9.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/lwt": "*",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/ctypes": "*",
        "@opam-alpha/ctypes-foreign": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "PREFIX=$cur__install"
          ],
          [
            "make",
            "PREFIX=$cur__install",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "zbar_version": {
            "val": "0.9.0",
            "scope": "global"
          },
          "zbar_installed": {
            "val": "true",
            "scope": "global"
          },
          "zbar_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/vbmithr/ocaml-zbar/archive/0.9.tar.gz",
        "checksum": "76c2c625cc5b8c4f258a0202a2acb283",
        "files": []
      }
    }
  }
}