{
  "versions": {
    "0.0.1": {
      "name": "@opam-alpha/pvem",
      "version": "0.0.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "please.ml",
            "build"
          ],
          [
            "ocaml",
            "please.ml",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "pvem_version": {
            "val": "0.0.1",
            "scope": "global"
          },
          "pvem_installed": {
            "val": "true",
            "scope": "global"
          },
          "pvem_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://bitbucket.org/smondet/pvem/get/pvem.0.0.1.tar.gz",
        "checksum": "33f982b96c6621543cba72ff9471b059",
        "files": []
      }
    }
  }
}