{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/valentine",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/cmdliner": "",
        "@opam-alpha/markup": "",
        "@opam-alpha/oasis": "  >= 0.4.0",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "oasis",
            "setup"
          ],
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "valentine_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "valentine_installed": {
            "val": "true",
            "scope": "global"
          },
          "valentine_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/fxfactorial/valentine/archive/v1.0.0.tar.gz",
        "checksum": "75c51f1b07f0f56fb647a09c26ea4610",
        "files": [
          {
            "name": "_oasis_remove_.ml",
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n"
          },
          {
            "name": "valentine.install",
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n"
          }
        ]
      }
    }
  }
}