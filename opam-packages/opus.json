{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/opus",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ogg": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "opus_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "opus_installed": {
            "val": "true",
            "scope": "global"
          },
          "opus_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://downloads.sourceforge.net/project/savonet/ocaml-opus/0.1.0/ocaml-opus-0.1.0.tar.gz",
        "checksum": "6d8e7d63c948e2a30795c58ce2633588",
        "files": []
      }
    },
    "0.1.1": {
      "name": "@opam-alpha/opus",
      "version": "0.1.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ogg": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "./configure",
            "CFLAGS=-I/usr/local/include",
            "LDFLAGS=-L/usr/local/lib",
            "OCAMLFLAGS=-ccopt -I/usr/local/include -cclib -L/usr/local/lib",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "opus_version": {
            "val": "0.1.1",
            "scope": "global"
          },
          "opus_installed": {
            "val": "true",
            "scope": "global"
          },
          "opus_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/savonet/ocaml-opus/releases/download/0.1.1/ocaml-opus-0.1.1.tar.gz",
        "checksum": "c58b1ff9859ce31b56f5cba9d22f91a2",
        "files": []
      }
    }
  }
}