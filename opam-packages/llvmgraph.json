        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "llvmgraph_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "llvmgraph_installed": {
            "val": "true",
            "scope": "global"
          },
          "llvmgraph_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
