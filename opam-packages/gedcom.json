          "gedcom_installed": {
            "val": "true",
            "scope": "global"
          },
          "gedcom_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/sagotch/gedcom.ml/archive/v1.0.0.tar.gz",
