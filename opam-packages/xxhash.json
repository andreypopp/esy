{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/xxhash",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/oasis": "",
        "@opam-alpha/ctypes": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "xxhash_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "xxhash_installed": {
            "val": "true",
            "scope": "global"
          },
          "xxhash_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/314eter/ocaml-xxhash/archive/v0.1.tar.gz",
        "checksum": "c5142adfe682acb70a9b1705e920c0ef",
        "files": []
      }
    }
  }
}