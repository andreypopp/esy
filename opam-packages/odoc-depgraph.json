{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/odoc-depgraph",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamldot": " >= 1.0.0"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "findlib-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "odoc_depgraph_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "odoc_depgraph_installed": {
            "val": "true",
            "scope": "global"
          },
          "odoc_depgraph_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/zoggy/odoc_depgraph/archive/v1.0.tar.gz",
        "checksum": "c53409245b7085aade0d9601fad50923",
        "files": []
      }
    }
  }
}