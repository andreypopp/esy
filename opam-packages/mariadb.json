{
  "versions": {
    "0.5.0": {
      "name": "@opam-alpha/mariadb",
      "version": "0.5.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ctypes": " >= 0.7.0",
        "@opam-alpha/ctypes-foreign": " >= 0.4.0",
        "@opam-alpha/async": "*",
        "@opam-alpha/lwt": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "mariadb_version": {
            "val": "0.5.0",
            "scope": "global"
          },
          "mariadb_installed": {
            "val": "true",
            "scope": "global"
          },
          "mariadb_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/andrenth/ocaml-mariadb/archive/0.5.0.tar.gz",
        "checksum": "6bf438d6e74586bc6cc61d00b11f8ed1",
        "files": []
      }
    },
