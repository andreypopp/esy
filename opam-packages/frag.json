{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/frag",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/core": " >= 109.42.0",
        "@opam-alpha/mpp": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/re": "*",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "frag_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "frag_installed": {
            "val": "true",
            "scope": "global"
          },
          "frag_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://pw374.github.io/distrib/frag/frag-0.1.0.tar.gz",
        "checksum": "934aeba80b96ef75d5bd8b366e7b1a18",
        "files": [
          {
            "name": "frag.install",
            "content": "bin: [\n  \"?_build/src/fragment_main.byte\" {\"frag\"}\n  \"?_build/src/fragment_main.native\" {\"frag\"}\n]\n"
          }
        ]
      }
    }
  }
}