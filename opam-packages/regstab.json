{
  "versions": {
    "2.0.0": {
      "name": "@opam-alpha/regstab",
      "version": "2.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "all"
          ],
          [
            "make",
            "opt"
          ],
          [
            "make",
            "install",
            "PREFIX=$cur__install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "regstab_version": {
            "val": "2.0.0",
            "scope": "global"
          },
          "regstab_installed": {
            "val": "true",
            "scope": "global"
          },
          "regstab_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/859/regstab-2.0.0.tar.gz",
        "checksum": "bacea0b985f75328453a2b2da40afa07",
        "files": [
          {
            "name": "opam.patch",
            "content": "diff -ru regstab.2.0.0/myocamlbuild.ml regstab.2.0.0/myocamlbuild.ml\n--- regstab.2.0.0/myocamlbuild.ml\t2012-05-04 05:14:37.000000000 +0200\n+++ regstab.2.0.0/myocamlbuild.ml\t2012-10-05 18:14:59.193868570 +0200\n@@ -55,7 +55,7 @@\n   dirs#man/\"man1\"/\"sch2ltl.opt.1\" , \"man\"/\"man1\"/\"sch2ltl.opt.1\";\n   dirs#doc/\"manual.pdf\"           , \"doc\"/pkgbase;\n   dirs#doc/\"developer\"            , \"doc\"/pkgbase/\"developer_doc\";\n-  dirs#tools                      , \"share\"/pkgbase;\n+  dirs#tools                      , \"lib\"/pkgbase/\"share\"/pkgbase;\n   dirs#ex                         , \"doc\"/pkgbase;\n   dirs#bin/\"regstab\"              , \"bin\"/\"regstab\";\n   dirs#bin/\"regstab.opt\"          , \"bin\"/\"regstab.opt\";\n"
          },
          {
            "name": "regstab.install",
            "content": "bin: [\n  \"_build/src/executables/sch2ltl.native\" {\"sch2ltl.opt\"}\n  \"_build/src/executables/sch2ltl.byte\" {\"sch2ltl\"}\n  \"_build/src/executables/sch2cnf.native\" {\"sch2cnf.opt\"}\n  \"_build/src/executables/sch2cnf.byte\" {\"sch2cnf\"}\n  \"_build/src/executables/main.native\" {\"regstab.opt\"}\n  \"_build/src/executables/main.byte\" {\"regstab\"}\n]\n"
          }
        ]
      }
    }
  }
