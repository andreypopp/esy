{
  "versions": {
    "0.1.3": {
      "name": "@opam-alpha/lilis",
      "version": "0.1.3",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/batteries": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/menhir": "*",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/cmdliner": "*",
        "@opam-alpha/cairo2": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install",
            "--%{cairo2+lablgtk:enable}%-cairo",
            "--disable-tyxml",
            "--${cmdliner_enable:-disable}-glilis-ex"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "lilis_version": {
            "val": "0.1.3",
            "scope": "global"
          },
          "lilis_installed": {
            "val": "true",
            "scope": "global"
          },
          "lilis_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/Drup/LILiS/archive/v0.1.3.tar.gz",
        "checksum": "abd51ba405128f13befd16f03e4f0d9f",
        "files": [
          {
            "name": "lilis.install",
            "content": "bin: [\"?glilis_ex.native\" {\"glilis\"}]\n"
          }
        ]
      }
    },
