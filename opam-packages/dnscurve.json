        "@opam-alpha/lwt": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "PREFIX=$cur__install"
          ],
          [
            "make",
            "PREFIX=$cur__install",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "dnscurve_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "dnscurve_installed": {
            "val": "true",
            "scope": "global"
          },
          "dnscurve_enable": {
            "val": "enable",
