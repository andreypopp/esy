          "rubytt_installed": {
            "val": "true",
            "scope": "global"
          },
          "rubytt_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/chenyukang/rubytt/archive/v0.1.tar.gz",
        "checksum": "b42dfee18d5d061583736a3b644133e6",
        "files": [
          {
            "name": "rubytt.install",
            "content": "bin: [\"bin/main.native\" {\"rubytt\"}]"
          }
        ]
      }
    }
  }
}