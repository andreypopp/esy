{
  "versions": {
    "2.0.8": {
      "name": "@opam-alpha/aifad",
      "version": "2.0.8",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base-threads": "",
        "@opam-alpha/cfg": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/pcre": "",
        "@opam-alpha/res": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "aifad_version": {
            "val": "2.0.8",
            "scope": "global"
          },
          "aifad_installed": {
            "val": "true",
            "scope": "global"
          },
          "aifad_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mmottl/aifad/releases/download/v2.0.8/aifad-2.0.8.tar.gz",
        "checksum": "d4d4d11f08b3f0d719580b643510de75",
        "files": [
          {
            "name": "_oasis_remove_.ml",
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n"
          },
          {
            "name": "aifad.install",
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n"
          }
        ]
      }
    }
  }
}