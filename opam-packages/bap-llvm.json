{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-llvm",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/bap-std": "*",
        "@opam-alpha/cmdliner": "*",
        "@opam-alpha/conf-bap-llvm": "*",
        "@opam-alpha/conf-llvm": "*",
        "@opam-alpha/conf-env-travis": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--with-cxx=`which clang++`",
            "--with-llvm-version=${conf_bap_llvm_version}",
            "--with-llvm-version=${conf_llvm_version}",
            "--with-llvm-config=",
            "--with-llvm-config=",
            "--enable-llvm"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_llvm_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_llvm_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_llvm_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.0.0.tar.gz",
        "checksum": "07dce66dd871e448652d8885283c6631",
        "files": [
          {
            "name": "detect.travis",
            "content": "#!/bin/sh\n\ncat > bap-llvm.config <<EOF\n travis : ${TRAVIS:-false}\nEOF\n"
          }
        ]
      }
    },
