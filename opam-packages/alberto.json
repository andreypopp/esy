{
  "versions": {
    "0.4.0": {
      "name": "@opam-alpha/alberto",
      "version": "0.4.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocplib-endian": "*",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "alberto_version": {
            "val": "0.4.0",
            "scope": "global"
          },
          "alberto_installed": {
            "val": "true",
            "scope": "global"
          },
          "alberto_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://codeload.github.com/selectel/alberto/tar.gz/0.4",
        "checksum": "2bf9cc79f6e164567dd837d51aed3487",
        "files": []
      }
    }
  }
