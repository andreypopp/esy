{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/tree_layout",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/tyxml": "",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--enable-tests",
            "--enable-benchmark",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "tree___layout_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "tree___layout_installed": {
            "val": "true",
            "scope": "global"
          },
          "tree___layout_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/Drup/tree_layout/archive/0.1.0.tar.gz",
        "checksum": "eaf12c96b8d60b53d535434e8439c417",
        "files": []
      }
    }
  }
}