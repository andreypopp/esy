{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/notty",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/result": "*",
        "@opam-alpha/uucp": "=1.1.0",
        "@opam-alpha/uuseg": "*",
        "@opam-alpha/uutf": " <= 0.9.4",
        "@opam-alpha/lwt": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install",
            "--enable-unix",
            "--${lwt_enable:-disable}-lwt"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "notty_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "notty_installed": {
            "val": "true",
            "scope": "global"
          },
          "notty_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/pqwy/notty/archive/v0.1.0.tar.gz",
        "checksum": "5aa0780bbc1f10fd20bd14ea8486276e",
        "files": []
      }
    },
