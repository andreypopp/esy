{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/brozip",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/brotli": "",
        "@opam-alpha/cmdliner": "",
        "@opam-alpha/lwt": "",
        "@opam-alpha/oasis": "  >= 0.4.0",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "oasis",
            "setup"
          ],
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "brozip_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "brozip_installed": {
            "val": "true",
            "scope": "global"
          },
          "brozip_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/fxfactorial/brozip/archive/v1.0.tar.gz",
        "checksum": "c9cf68cc8a57ce6498bcfa32d69dc5f5",
        "files": [
          {
            "name": "_oasis_remove_.ml",
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n"
          },
          {
            "name": "brozip.install",
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n"
          }
        ]
      }
    },
