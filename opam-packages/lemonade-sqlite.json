{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/lemonade-sqlite",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/broken": " >= 0.4.2",
        "@opam-alpha/bsdowl": " >= 3.0.0",
        "@opam-alpha/conf-bmake": "*",
        "@opam-alpha/lemonade": " >= 0.5.0",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/sqlite3": " >= 2.0.9"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "conf-bmake:path",
            "-Ibsdowl__share",
            "all"
          ],
          [
            "conf-bmake:path",
            "-Ibsdowl__share",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "lemonade_sqlite_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "lemonade_sqlite_installed": {
            "val": "true",
            "scope": "global"
          },
          "lemonade_sqlite_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/michipili/lemonade-sqlite/releases/download/v0.1.0/lemonade-sqlite-0.1.0.tar.xz",
        "checksum": "43d9fb773d9129610c7edd8aa961f719",
        "files": []
      }
    }
  }
}