{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/ocamlcc",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlclean": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "-prefix",
            "$cur__install"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocamlcc_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "ocamlcc_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocamlcc_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://raw.github.com/ocaml-bytes/ocamlcc/master/dist/ocamlcc-1.0.tar.bz2",
        "checksum": "7b065f5c7b3f0b29aebb5f15f8d3ecbe",
        "files": []
      }
    }
  }
}