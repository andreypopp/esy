{
  "versions": {
    "0.3.0": {
      "name": "@opam-alpha/stdint",
      "version": "0.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/base-bytes": "*",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "stdint_version": {
            "val": "0.3.0",
            "scope": "global"
          },
          "stdint_installed": {
            "val": "true",
            "scope": "global"
          },
          "stdint_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/andrenth/ocaml-stdint/archive/0.3.0.tar.gz",
        "checksum": "1f9fa21baf9c0df490a841a6e916963a",
        "files": []
      }
    }
  }
}