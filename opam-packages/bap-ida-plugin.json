{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/bap-ida-plugin",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/bap": "*",
        "@opam-alpha/cmdliner": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install",
            "--enable-ida-plugin"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_ida_plugin_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bap_ida_plugin_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_ida_plugin_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.0.0.tar.gz",
        "checksum": "07dce66dd871e448652d8885283c6631",
        "files": []
      }
    },
