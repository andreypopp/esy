{
  "versions": {
    "1.1.0": {
      "name": "@opam-alpha/obytelib",
      "version": "1.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlbuild": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "-prefix",
            "$cur__install"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "obytelib_version": {
            "val": "1.1.0",
            "scope": "global"
          },
          "obytelib_installed": {
            "val": "true",
            "scope": "global"
          },
          "obytelib_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/bvaugon/obytelib/archive/1.1.tar.gz",
        "checksum": "d5ed657b5bc6a31f36d7b263043db7bf",
        "files": []
      }
    }
  }
}