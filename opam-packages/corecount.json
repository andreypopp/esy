{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/corecount",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/oasis": "  >= 0.4.0",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "oasis",
            "setup"
          ],
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "corecount_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "corecount_installed": {
            "val": "true",
            "scope": "global"
          },
          "corecount_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/fxfactorial/ocaml-corecount/archive/v1.0.0.tar.gz",
        "checksum": "e73e83bfaa0fc9f2434ebbd8cad590b6",
        "files": []
      }
    }
  }
}