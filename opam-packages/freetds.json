      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./autogen.sh"
          ],
          [
            "./configure",
            "--prefix",
            "$cur__install",
            "--sbindir=$cur__lib/freetds/sbin",
            "--libexecdir=$cur__lib/freetds/libexec",
            "--sysconfdir=$cur__lib/freetds/etc",
            "--sharedstatedir=$cur__lib/freetds/com",
            "--localstatedir=$cur__lib/freetds/var",
            "--libdir=$cur__lib/freetds/lib",
            "--includedir=$cur__lib/freetds/include",
            "--datarootdir=$cur__lib/freetds/share"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "freetds_version": {
            "val": "0.4.0",
            "scope": "global"
