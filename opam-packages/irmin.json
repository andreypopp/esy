{
  "versions": {
    "0.12.0": {
      "name": "@opam-alpha/irmin",
      "version": "0.12.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlbuild": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/topkg": "  >= 0.7.8",
        "@opam-alpha/ezjsonm": " >= 0.4.2  < 0.5.0",
        "@opam-alpha/fmt": "*",
        "@opam-alpha/ocamlgraph": "*",
        "@opam-alpha/lwt": " >= 2.4.7",
        "@opam-alpha/logs": " >= 0.5.0",
        "@opam-alpha/cstruct": " >= 1.6.0",
        "@opam-alpha/mirage-tc": " >= 0.3.0",
        "@opam-alpha/mstruct": "*",
        "@opam-alpha/uri": " >= 1.3.12",
        "@opam-alpha/astring": "*",
        "@opam-alpha/hex": "*",
        "@opam-alpha/re": "*",
        "@opam-alpha/cmdliner": "*",
        "@opam-alpha/crunch": "*",
        "@opam-alpha/mtime": "",
        "@opam-alpha/git-unix": "*",
        "@opam-alpha/cohttp": "*",
        "@opam-alpha/alcotest": "  >= 0.4.1",
        "@opam-alpha/irmin-watcher": "*",
        "@opam-alpha/git": "*",
        "@opam-alpha/mirage-git": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "pkg/pkg.ml",
            "build",
            "--pinned",
            "false",
            "--with-http",
            "${cohttp_installed:-false}",
            "--with-git",
            "${git_installed:-false}",
            "--with-unix",
            "%{irmin-watcher+git-unix:installed}%",
            "--with-mirage",
            "${mirage_git_installed:-false}"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "irmin_version": {
            "val": "0.12.0",
            "scope": "global"
          },
          "irmin_installed": {
            "val": "true",
            "scope": "global"
          },
          "irmin_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mirage/irmin/releases/download/0.12.0/irmin-0.12.0.tbz",
        "checksum": "04158d3e093add7b556323aeb117219c",
        "files": []
      }
    }
  }
}