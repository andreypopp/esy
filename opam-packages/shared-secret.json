{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/shared-secret",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ounit": "",
        "@opam-alpha/oasis": "",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "oasis",
            "setup"
          ],
          [
            "./configure",
            "--prefix=$cur__install",
            "--${ounit_enable:-disable}-tests"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "shared_secret_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "shared_secret_installed": {
            "val": "true",
            "scope": "global"
          },
          "shared_secret_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/marcoonroad/shared-secret/archive/v0.1.zip",
        "checksum": "3ac49e697da692e74f8f20a4bea50dbe",
        "files": []
      }
    }
  }
}