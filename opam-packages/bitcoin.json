    "1.0.0": {
      "name": "@opam-alpha/bitcoin",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/yojson": "*",
        "@opam-alpha/ocamlnet": " < 4.0.0",
        "@opam-alpha/ocsigenserver": "*",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "doc"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bitcoin_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "bitcoin_installed": {
            "val": "true",
            "scope": "global"
          },
          "bitcoin_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
