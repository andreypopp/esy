          },
          "sqlite3EZ_installed": {
            "val": "true",
            "scope": "global"
          },
          "sqlite3EZ_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mlin/ocaml-sqlite3EZ/archive/v0.1.0.tar.gz",
