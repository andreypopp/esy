        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "rm",
            "setup.ml"
          ],
          [
            "oasis",
            "setup"
          ],
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "optimization1d_version": {
            "val": "0.5.0",
            "scope": "global"
          },
          "optimization1d_installed": {
            "val": "true",
            "scope": "global"
          },
          "optimization1d_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
