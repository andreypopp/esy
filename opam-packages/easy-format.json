    "1.0.1": {
      "name": "@opam-alpha/easy-format",
      "version": "1.0.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "easy_format_version": {
            "val": "1.0.1",
            "scope": "global"
          },
          "easy_format_installed": {
            "val": "true",
            "scope": "global"
          },
          "easy_format_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/mjambon/easy-format/archive/v1.0.1.tar.gz",
        "checksum": "ecaf5d060f7cf8961631ab319e40677c",
        "files": [
          {
            "name": "easy-format-make.diff",
            "content": "--- Makefile.orig\t2012-10-12 16:53:00.101688369 -0700\n+++ Makefile\t\t2012-10-12 17:02:33.061706704 -0700\n@@ -6,10 +6,13 @@\n all:\n\tocamlc -c easy_format.mli\n \tocamlc -c -dtypes easy_format.ml\n+\tocamlmklib -o easy_format easy_format.cmo\n \ttouch bytecode\n opt:\n\tocamlc -c easy_format.mli\n \tocamlopt -c -dtypes easy_format.ml\n+\tocamlmklib -o easy_format easy_format.cmx\n+\tocamlopt -I . -shared -linkall -o easy_format.cmxs easy_format.cmx\n \ttouch nativecode\n test: all simple_example.out\n       ocamlc -o test_easy_format -dtypes easy_format.cmo test_easy_format.ml\n@@ -45,8 +48,8 @@\n \n \n COMMON_INSTALL_FILES = META easy_format.cmi easy_format.mli\n-BC_INSTALL_FILES = easy_format.cmo \n-NC_INSTALL_FILES = easy_format.cmx easy_format.o\n+BC_INSTALL_FILES = easy_format.cmo easy_format.cma\n+NC_INSTALL_FILES = easy_format.cmx easy_format.cmxa easy_format.cmxs easy_format.o easy_format.a\n \n install:\n\techo \"version = \\\"$(VERSION)\\\"\" > META; cat META.tpl >> META\n"
          },
          {
            "name": "meta-tpl.diff",
            "content": "--- META.tpl.orig\t2012-10-12 17:46:54.701791877 -0700\n+++ META.tpl\t\t2012-10-12 17:47:10.653792387 -0700\n@@ -1,4 +1,5 @@\n name = \"easy-format\"\n description = \"Indentation made easy(ier)\"\n-archive(byte)    = \"easy_format.cmo\"\n-archive(native)  = \"easy_format.cmx\"\n+archive(byte)    = \"easy_format.cmo\"\n+archive(native)  = \"easy_format.cmx\"\n+archive(native,plugin)  = \"easy_format.cmxs\"\n"
          }
        ]
      }
    },
