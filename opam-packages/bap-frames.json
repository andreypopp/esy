{
  "versions": {
    "2.1.0": {
      "name": "@opam-alpha/bap-frames",
      "version": "2.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/oasis": " =0.4.7",
        "@opam-alpha/bap-std": "*",
        "@opam-alpha/bap-traces": "*",
        "@opam-alpha/cmdliner": "*",
        "@opam-alpha/piqi": " >= 0.7.4"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "oasis",
            "setup"
          ],
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "bap_frames_version": {
            "val": "2.1.0",
            "scope": "global"
          },
          "bap_frames_installed": {
            "val": "true",
            "scope": "global"
          },
          "bap_frames_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap-frames/archive/v2.1.0.tar.gz",
        "checksum": "9230dfa0eb2e1270be05fe20a3ccd001",
        "files": []
      }
    }
  }
}