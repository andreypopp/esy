{
  "versions": {
    "0.3.0": {
      "name": "@opam-alpha/ocamlwc",
      "version": "0.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocamlwc_version": {
            "val": "0.3.0",
            "scope": "global"
          },
          "ocamlwc_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocamlwc_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://www.lri.fr/~filliatr/ftp/ocaml/misc/ocamlwc-0.3.tar.gz",
        "checksum": "72bb9e8c0dad08735b62dabe216967d6",
        "files": []
      }
    }
  }
}