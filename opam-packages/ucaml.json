{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/ucaml",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ucaml_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "ucaml_installed": {
            "val": "true",
            "scope": "global"
          },
          "ucaml_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/masateruk/micro-caml/archive/v0.1.tar.gz",
        "checksum": "d908ba04acf795c410616e83bfd663b6",
        "files": [
          {
            "name": "ucaml.install",
            "content": "bin: [\n  \"ucaml\"\n]\n"
          }
        ]
      }
    }
  }
}