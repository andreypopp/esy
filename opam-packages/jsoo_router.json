{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/jsoo_router",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/js_of_ocaml": " >= 2.8.1  < 3.0.0",
        "@opam-alpha/lwt": " >= 2.5.2",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "jsoo___router_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "jsoo___router_installed": {
            "val": "true",
            "scope": "global"
          },
          "jsoo___router_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/xvw/jsoo_router/archive/v1.0.tar.gz",
        "checksum": "cd1dd766082506116da29af9ce6573f6",
        "files": []
      }
    }
  }
}