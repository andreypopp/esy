{
  "versions": {
    "0.7.1": {
      "name": "@opam-alpha/zenon",
      "version": "0.7.1",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/coq": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "mkdir",
            "-p",
            "zenon__lib"
          ],
          [
            "./configure",
            "--prefix",
            "$cur__install",
            "--tools_prefix",
            "zenon__lib"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "zenon_version": {
            "val": "0.7.1",
            "scope": "global"
          },
          "zenon_installed": {
            "val": "true",
            "scope": "global"
          },
          "zenon_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://pkgs.fedoraproject.org/repo/pkgs/zenon/zenon-0.7.1.tar.xz/cb8b35e2d55dabcc7d8a1e8b3600f68a/zenon-0.7.1.tar.xz",
        "checksum": "cb8b35e2d55dabcc7d8a1e8b3600f68a",
        "files": []
      }
    },
