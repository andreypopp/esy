{
  "versions": {
    "1.0.0": {
      "name": "@opam-alpha/cordova-plugin-device-motion",
      "version": "1.0.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/gen_js_api": "*",
        "@opam-alpha/ocaml-js-stdlib": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "build"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "cordova_plugin_device_motion_version": {
            "val": "1.0.0",
            "scope": "global"
          },
          "cordova_plugin_device_motion_installed": {
            "val": "true",
            "scope": "global"
          },
          "cordova_plugin_device_motion_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/dannywillems/ocaml-cordova-plugin-device-motion/archive/v1.0.tar.gz",
        "checksum": "8c976b5e24d44aaaee220bb01989251c",
        "files": []
      }
    }
  }
}