{
  "versions": {
    "0.0.2": {
      "name": "@opam-alpha/ocaml-xdg-basedir",
      "version": "0.0.2",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/fileutils": "*",
        "@opam-alpha/ounit": "*",
        "@opam-alpha/ocamlbuild": ""
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "ocaml",
            "setup.ml",
            "-configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "ocaml",
            "setup.ml",
            "-build"
          ],
          [
            "ocaml",
            "setup.ml",
            "-install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocaml_xdg_basedir_version": {
            "val": "0.0.2",
            "scope": "global"
          },
          "ocaml_xdg_basedir_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocaml_xdg_basedir_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/902/ocaml-xdg-basedir-0.0.2.tar.gz",
        "checksum": "6ecdb1fbf5bc307c8fd29afe72ec7d76",
        "files": []
      }
    }
  }
}