{
  "versions": {
    "1.1.0": {
      "name": "@opam-alpha/annot",
      "version": "1.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "annot_version": {
            "val": "1.1.0",
            "scope": "global"
          },
          "annot_installed": {
            "val": "true",
            "scope": "global"
          },
          "annot_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/avsm/ocaml-annot/archive/v1.1.0.tar.gz",
        "checksum": "f9b293b1317fe6968e1f41e5cda2a41c",
        "files": []
      }
    }
  }
}