{
  "versions": {
    "1.3.0": {
      "name": "@opam-alpha/acgtk",
      "version": "1.3.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/dypgen": "*",
        "@opam-alpha/bolt": "*",
        "@opam-alpha/ANSITerminal": " >= 0.7.0",
        "@opam-alpha/cairo2": "*",
        "@opam-alpha/ocf": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix=$cur__install"
          ],
          [
            "make",
            "all"
          ],
          [
            "make",
            "install-all"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "acgtk_version": {
            "val": "1.3.0",
            "scope": "global"
          },
          "acgtk_installed": {
            "val": "true",
            "scope": "global"
          },
          "acgtk_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://calligramme.loria.fr/acg/software/acg-1.3.0-20160909.tar.gz",
        "checksum": "829d35110953a6ed94d9ba2aee84b05d",
        "files": []
      }
    }
  }
}