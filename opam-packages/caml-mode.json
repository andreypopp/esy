{
  "versions": {
    "4.2.0": {
      "name": "@opam-alpha/caml-mode",
      "version": "4.2.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/conf-emacs": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "make",
            "install",
            "EMACSDIR=$cur__share/emacs/site-lisp"
          ],
          [
            "make",
            "install-ocamltags",
            "BINDIR=$cur__bin"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "caml_mode_version": {
            "val": "4.2.0",
            "scope": "global"
          },
          "caml_mode_installed": {
            "val": "true",
            "scope": "global"
          },
          "caml_mode_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "https://github.com/ocaml/tuareg/releases/download/2.0.9/caml-mode-4.02.tar.gz",
        "checksum": "5f6d202df55c7982001c01440ddd2a8e",
        "files": []
      }
    }
  }
}