{
  "versions": {
    "0.1.0": {
      "name": "@opam-alpha/ocplib-concur",
      "version": "0.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/ocamlfind": "esy-ocaml/ocamlfind#esy/1.7.1-esy3",
        "@opam-alpha/ocp-build": " >= 1.99.14",
        "@opam-alpha/base-unix": "*",
        "@opam-alpha/lwt": " >= 2.3.2",
        "@opam-alpha/ocplib-endian": "*",
        "@opam-alpha/async": "*"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "--prefix",
            "$cur__install"
          ],
          [
            "make"
          ],
          [
            "make",
            "install"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "ocplib_concur_version": {
            "val": "0.1.0",
            "scope": "global"
          },
          "ocplib_concur_installed": {
            "val": "true",
            "scope": "global"
          },
          "ocplib_concur_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://github.com/OCamlPro/ocplib-concur/archive/0.1.tar.gz",
        "checksum": "a0651e7c36ba6a2a7463c28458c869c1",
        "files": []
      }
    }
  }
}