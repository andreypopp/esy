{
  "versions": {
    "1.1.0": {
      "name": "@opam-alpha/fm-simplex-plugin",
      "version": "1.1.0",
      "dependencies": {
        "substs": "esy-ocaml/substs",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin",
        "@opam-alpha/zarith": "*",
        "@opam-alpha/alt-ergo": "=1.1.0"
      },
      "peerDependencies": {
        "ocaml": "*"
      },
      "esy": {
        "build": [
          [
            "./configure",
            "-prefix",
            "$cur__install"
          ],
          [
            "make",
            "fm-simplex"
          ],
          [
            "make",
            "install-fm-simplex",
            "MANDIR=$cur__man/man1"
          ],
          [
            "sh",
            "-c",
            "(opam-installer --prefix=$cur__install || true)"
          ]
        ],
        "buildsInSource": true,
        "exportedEnv": {
          "fm_simplex_plugin_version": {
            "val": "1.1.0",
            "scope": "global"
          },
          "fm_simplex_plugin_installed": {
            "val": "true",
            "scope": "global"
          },
          "fm_simplex_plugin_enable": {
            "val": "enable",
            "scope": "global"
          }
        }
      },
      "opam": {
        "url": "http://alt-ergo.ocamlpro.com/http/alt-ergo-1.01/alt-ergo-1.01.tar.gz",
        "checksum": "9fbf8e42bec2a171dc9422f9dcea9519",
